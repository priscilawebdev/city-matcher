# editorconfig.org

root = true

[*]
indent_style = tab
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.{sass,scss,json}]
indent_style = space
indent_size = 2

[*.js]
line_length=100
indent_style = tab

